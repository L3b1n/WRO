Sub alignment 'функция выранивание на ворота противника
  While Sensor.ReadRawValue(Port_front_color,0)<=2700
    If K>=35 then
      I=0
      runD=100
      While runD>28 
        Motor.StartPower("D",-100)
        cof_OY=1
        cof_OX=0
        condition=0
        condition_x=0
        condition_y=1
        type_odometriy=1
        A=-100
        B=100
        C=0
        speed_purpose()
        If I=5 then
          runD=Math.Abs(Motor.GetSpeed("D"))
        Else
          I=I+1
        EndIf
        Program.Delay(5)
      EndWhile
      I=0
      runD=100
      While runD>28
        Motor.StartPower("D",100)
        cof_OY=1
        cof_OX=0
        condition=0
        condition_x=0
        condition_y=1
        type_odometriy=1
        A=-100
        B=100
        C=0
        speed_purpose()
        If I=5 then
          runD=Math.Abs(Motor.GetSpeed("D"))
